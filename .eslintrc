{
  "extends": "airbnb",
  "globals": {
    "window": true,
    "document": true
  },
  "rules": {
    "import/first": [0],
    "no-underscore-dangle": [0],
    "space-infix-ops": [1],
    "comma-dangle": [0],
    "arrow-body-style": [0],
    "react/forbid-prop-types": [1],
    "react/self-closing-comp": [1],
    "max-len": [1],
    "react/jsx-filename-extension": [0],
    "arrow-parens": [0],
    "brace-style": [0],
    "array-bracket-spacing": [0],
    "no-nested-ternary": [0],
    "prefer-template": [1],
    "no-tabs": [1],
    "react/jsx-indent": [1]
  }
}
